<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Library</OutputType>
    <Version>8.0.1</Version>
    <TargetFramework>net8.0</TargetFramework>
    <AssemblyName>Aiursoft.GitRunner</AssemblyName>
    <RootNamespace>Aiursoft.GitRunner</RootNamespace>
    <IsTestProject>false</IsTestProject>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <IsPackable>true</IsPackable>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Company>Aiursoft</Company>
    <Product>GitRunner</Product>
    <Description>Nuget package of 'GitRunner' provided by Aiursoft</Description>
    <PackageId>Aiursoft.GitRunner</PackageId>
    <PackageTags>nuget package dotnet csproj dependencies git runner</PackageTags>
    <PackageReadmeFile>README.md</PackageReadmeFile>
  </PropertyGroup>
  <ItemGroup>
    <None Include="..\..\README.md" Pack="true" PackagePath="." />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Aiursoft.Canon" Version="8.0.0" />
    <PackageReference Include="Aiursoft.CSTools" Version="8.0.0" />
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="8.0.0" />
  </ItemGroup>
</Project>